generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum Role {
    ADMIN
    MANAGER
    SUPPORT
}

model User {
    id       String @id @default(uuid())
    username String @unique
    password String
    role     Role   @default(SUPPORT)
}
